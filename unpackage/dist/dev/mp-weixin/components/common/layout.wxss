
.lay-line {
    background-color: #FFFFFF;
    padding: 12px 5px 10px 5px;
    box-sizing: border-box;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    border-bottom: 1px solid #EEEEEE;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
            justify-content: space-between;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
}
.lay-verline {
    width: 2px;
    margin: 2px 5px;
}
.lay-verline + view{
    color: #000000;
}
.lay-card {
    font-size: 13px;
    background-color: #FFFFFF;
    padding: 11px;
    box-sizing: border-box;
    margin-bottom: 10px;
}
.lay-min-top {
    padding-top: 3px;
}
.lay-left-con {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
}

